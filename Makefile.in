all: dbgen_exe util_exe

dbgen_exe:
	cd datagen/dbgen; $(MAKE)

util_exe:
	cd dbdriver/utils; $(MAKE)

clean:
	cd datagen/dbgen; $(MAKE) clean
	cd dbdriver/utils; $(MAKE) clean

install:
	chmod +x data_collect/@DATABASE_TO_USE@/*.sh
	chmod +x scripts/@DATABASE_TO_USE@/*.sh
	chmod +x dbdriver/scripts/@DATABASE_TO_USE@/*.sh
	chmod +x dbdriver/scripts/*.pl
