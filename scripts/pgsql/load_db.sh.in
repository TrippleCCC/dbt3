TABLESPACES=${1}

BGIT=
if [ ${TABLESPACES} -eq 1 ]; then
	BGIT="&"
fi

echo "Loading table tables... "

date
@PSQL@ -d $SID -c "COPY supplier FROM '$DSS_PATH/supplier.tbl' USING DELIMITERS '|';" || exit 1 ${BGIT}
@PSQL@ -d $SID -c "COPY part FROM '$DSS_PATH/part.tbl' USING DELIMITERS '|';" || exit 1 ${BGIT}
@PSQL@ -d $SID -c "COPY partsupp FROM '$DSS_PATH/partsupp.tbl' USING DELIMITERS '|';" || exit 1 ${BGIT}
@PSQL@ -d $SID -c "COPY customer FROM '$DSS_PATH/customer.tbl' USING DELIMITERS '|';" || exit 1 ${BGIT}
@PSQL@ -d $SID -c "COPY orders FROM '$DSS_PATH/orders.tbl' USING DELIMITERS '|';" || exit 1 ${BGIT}
@PSQL@ -d $SID -c "COPY lineitem FROM '$DSS_PATH/lineitem.tbl' USING DELIMITERS '|';" || exit 1 ${BGIT}
@PSQL@ -d $SID -c "COPY nation FROM '$DSS_PATH/nation.tbl' USING DELIMITERS '|';" || exit 1 ${BGIT}
@PSQL@ -d $SID -c "COPY region FROM '$DSS_PATH/region.tbl' USING DELIMITERS '|';" || exit 1 ${BGIT}

wait

exit 0
