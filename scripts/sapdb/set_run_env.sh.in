export DSS_QUERY=@TOPDIR@/datagen/@DATABASE_TO_USE@-queries
export DSS_PATH=/dbt3_data/sapdb
export DSS_CONFIG=@TOPDIR@/datagen/dbgen
export SID=DBT3
export DBT3_PERL_MODULE=@TOPDIR@/perlmodules
export DBT3_BACKUP=/dbt3_backup/dbt3.bak
export PATH=/opt/sapdb/depend/bin/:/opt/sapdb/indep_prog/bin/:$PATH
export SYS_DIR=/dbt3/pgsql
export DBUSER=dbt
